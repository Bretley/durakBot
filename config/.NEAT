[NEAT]
fitness_criterion      = mean
fitness_threshold      = 15
no_fitness_termination = False
pop_size               = 250
reset_on_extinction    = False

[DefaultGenome]
# Node Activation Options
activation_default     = random
activation_mutate_rate = 0.1
activation_options     = sigmoid gauss

# Node Aggregation Options
aggregation_default     = random
aggregation_mutate_rate = 0.1
aggregation_options     = sum product min max mean median maxabs

# Node Bias Options
bias_init_mean    = 0
bias_init_stdev   = 1.0
bias_init_type    = gaussian
bias_max_value    = 30.0
bias_min_value    = -30.0
bias_mutate_power = 0.5
bias_mutate_rate  = 0.25
bias_replace_rate = 0.1

# Genome Compatibility Options
compatibility_threshold            = 2.5
compatibility_disjoint_coefficient = 1.0
compatibility_weight_coefficient   = 0.5

# Connection Add/Remove Rates
conn_add_prob    = 0.5
conn_delete_prob = 0.2

# Connection Enable Options
enabled_default           = True
enabled_mutate_rate       = 0.2
enabled_rate_to_false_add = 0.1
enabled_rate_to_true_add  = 0.3

# Recurrence Options
feed_forward       = False
initial_connection = partial_direct 0.05

# Node Add/Remove Rates
node_add_prob    = 0.5
node_delete_prob = 0.2

# Network Parameters
num_hidden  = 0
num_inputs  = 37
num_outputs = 38

# Node Response Options
response_init_mean    = 0
response_init_stdev   = 1
response_init_type    = gaussian
response_max_value    = 30.0
response_min_value    = -30.0
response_mutate_power = 0.5
response_mutate_rate  = 0.25
response_replace_rate = 0.1

# Structural Mutation Options
single_structural_mutation = False
structural_mutation_surer  = True

# Connection Weight Options
weight_init_mean    = 0
weight_init_stdev   = 1.0
weight_init_type    = gaussian
weight_max_value    = 30
weight_min_value    = -30
weight_mutate_power = 0.5
weight_mutate_rate  = 0.25
weight_replace_rate = 0.1

[DefaultSpeciesSet]
compatibility_threshold = 2.5

[DefaultStagnation]
species_fitness_func = mean
max_stagnation       = 35
species_elitism      = 10

[DefaultReproduction]
elitism            = 5
survival_threshold = 0.2
min_species_size   = 5
