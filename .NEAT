[NEAT]
# NEAT Algorithm Options
fitness_criterion      = max
fitness_threshold      = 0.0
no_fitness_termination = False
pop_size               = 0
reset_on_extinction    = False

[DefaultStagnation]
# Default Stagnation Class Options
species_fitness_func = mean
max_stagnation       = 15
species_elitism      = 0

[DefaultReproduction]
# Default Reproduction Class Options
elitism            = 0
survival_threshold = 0.2
min_species_size   = 2

[DefaultGenome]
# Node Activation Options
activation_default      = random
activation_mutate_rate  = 0.0
activation_options      = sigmoid

# Node Aggregation Options
aggregation_default     = random
aggregation_mutate_rate = 0.0
aggregation_options     = sum

# Node Bias Options
bias_init_mean    = 0.0
bias_init_stdev   = 0.0
bias_init_type    = gaussian
bias_max_value    = 0.0
bias_min_value    = 0.0
bias_mutate_power = 0.0
bias_mutate_rate  = 0.0
bias_replace_rate = 0.0

# Genome Compatibility Options
compatibility_threshold            = 0.0
compatibility_disjoint_coefficient = 0.0
compatibility_weight_coefficient   = 0.0

# Connection Add/Remove Rates
conn_add_prob    = 0.0
conn_delete_prob = 0.0

# Connection Enable Options
enabled_default           = True
enabled_mutate_rate       = 0.00
enabled_rate_to_false_add = 0.00
enabled_rate_to_true_add  = 0.00

feed_forward       = True
initial_connection = unconnected

# Node Add/Remove rates
node_add_prob    = 0.0
node_delete_prob = 0.0

# Network Parameters
num_hidden  = 0
num_inputs  = 0
num_outputs = 0

# Node Response Options
response_init_mean    = 0.0
response_init_stdev   = 0.0
response_init_type    = gaussian
response_max_value    = 0.0
response_min_value    = 0.0
response_mutate_power = 0.0
response_mutate_rate  = 0.0
response_replace_rate = 0.0

single_structural_mutation = False
structural_mutation_surer  = default

# Connection Weight Options
weight_init_mean    = 0.0
weight_init_stdev   = 0.0
weight_init_type    = gaussian
weight_max_value    = 0.0
weight_min_value    = 0.0
weight_mutate_power = 0.5
weight_mutate_rate  = 0.8
weight_replace_rate = 0.1
